# "local" or "aws"
APP_MODE=local

# App log level "INFO", "DEBUG"
LOG_LEVEL=INFO

# AWS region used by Bedrock & S3
AWS_REGION=ap-southeast-1

# S3 content bucket, used when APP_MODE=aws
S3_BUCKET=axrail-bot-content
CONTENT_MANIFEST_KEY=current.json

# Bedrock model, used when APP_MODE=aws
BEDROCK_MODEL_ID=amazon.nova-lite-v1:0

# Crawler mode "local", "s3", "both"
INGESTION_MODE=local